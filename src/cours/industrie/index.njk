title: "Cours Industrie — TOLExperience (staging)"
layout: "layouts/base.njk"
---

<h1 class="tol-title">Industrie / Pro</h1>

<section class="grid">
  {% for page in collections.industrie | reverse %}
  <article class="card">
    <h3 class="tol-title">
      <a href="{{ page.url }}">{{ page.data.title }}</a>
    </h3>
    {% if page.data.intro %}
      <p class="tol-text">{{ page.data.intro }}</p>
    {% endif %}
    <p class="tol-text"><a href="{{ page.url }}">Ouvrir →</a></p>
  </article>
  {% endfor %}
</section>
